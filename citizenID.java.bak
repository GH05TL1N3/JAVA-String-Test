import java.util.Scanner;
public class citizenID {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		String userInput ;
		System.out.print("[+] Enter your citizen ID [Number Only]: > " );
		userInput = sc.next();
		System.out.print("[+] Result : > ");
		System.out.print(userInput.charAt(0) + " ");
		System.out.print(userInput.substring(1, 5) + " ");
		System.out.print(userInput.substring(5, 10) + " ");
		System.out.print(userInput.substring(10, 12) + " ");
		System.out.print(userInput.charAt(12) );


		System.out.print("\n\n");
		
	}
}
